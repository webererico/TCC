//RECEBE O COMANDO ARDUINO
void AcionaAR(float value) {
  if (value > 100.00 && value < 400.00) {
    if (value > 100.00 and value < 200.00) {
      value = value - 100.00;
      Serial.print("Ar 1, modo frio, temperatura:");
      enviaComando(1, 1 , value);
      Serial.println(value);
    } else if (value > 200 && value < 300) {
      value = value - 200;
      Serial.print("Ar 1, modo quente, temperatura:");
      Serial.println(value);
    } else if (value > 300) {
      value = value - 300;
      Serial.print("Ar 1, modo desumidificar, temperatura:");
      Serial.println(value);
    }
  } else if (value > 400 && value < 700) {
    if (value > 400 && value < 500) {
      value = value - 400;
      Serial.print("Ar 2, modo frio, temperatura:");
      Serial.println(value);
    } else if (value > 500 && value < 600) {
      value = value - 500;
      Serial.print("Ar 2, modo quente, temperatura:");
      Serial.println(value);
    } else if (value > 600) {
      value = value - 600;
      Serial.print("Ar 2, modo desumidificar, temperatura:");
      Serial.println(value);
    }
  } else if (value > 700 && value < 1000) {
    if (value > 700 && value < 800) {
      value = value - 700;
      Serial.print("Ar 3, modo frio, temperatura:");
      Serial.println(value);
    } else if (value > 800 && value < 900) {
      value = value - 800;
      Serial.print("Ar 3, modo quente, temperatura:");
      Serial.println(value);
    } else if (value > 900) {
      value = value - 900;
      Serial.print("Ar 3, modo desumidificar, temperatura:");
      Serial.println(value);
    }
  }

}


void setup() {
  //INICIANDO OS OBJETOS
  dht1.begin();
  dht2.begin();
  dht3.begin();
  dht4.begin();
  //INICIANDO A SERIAL
  pinMode(ledEnvioComando, OUTPUT);
  pinMode(botaoModo, INPUT);
  Serial.begin(9600);
}

void loop()
{
  //LEITURA DOS SENSORES
  float h1 = dht1.readHumidity();
  float t1 = dht1.readTemperature();
  float h2 = dht2.readHumidity();
  float t2 = dht2.readTemperature();
  float h3 = dht3.readHumidity();
  float t3 = dht3.readTemperature();
  float h4 = dht4.readHumidity();
  float t4 = dht4.readTemperature();

  //LEITURA DOS OFFSET E CALIBRAÃ‡ÃƒO
  valoroff1DHT1 = analogRead(offDHT1);
  valoroff2DHT2 = analogRead(offDHT2);
  valoroff3DHT3 = analogRead(offDHT3);
  valorcal1DHT1 = analogRead(calDHT1);
  valorcal2DHT2 = analogRead(calDHT2);
  valorcal3DHT3 = analogRead(calDHT3);

  //CONVERSAO VALORES (Regra de 3)
  valoroff1DHT1 = mapfloat(valoroff1DHT1, 40, 749, -1, 1);
  valoroff2DHT2 = mapfloat(valoroff2DHT2, 40, 749, -1, 1);
  valoroff3DHT3 = mapfloat(valoroff3DHT3, 40, 749, -1, 1);
  valorcal1DHT1 = mapfloat(valorcal1DHT1, 40, 749, 0.5, 1.5);
  valorcal2DHT2 = mapfloat(valorcal2DHT2, 40, 749, 0.5, 1.5);
  valorcal3DHT3 = mapfloat(valorcal3DHT3, 40, 749, 0.5, 1.5);


  if (isnan(t1) || isnan(h1) || isnan(t2) || isnan(h2) || isnan(t3) || isnan(h3) || isnan(t4) || isnan(h4)) {
    Serial.print("e");
    Serial.println();
  } else {
    static char buffer[32];
    static size_t pos;
    if (Serial.available() > 0) {
      char c = Serial.read();
      if (c == '\n') {  // on end of line, parse the number
        buffer[pos] = '\0';
        float value = atof(buffer);
        Serial.print(value);
//        AcionaAR(value);
        pos = 0;
      } else if (pos < sizeof buffer - 1) {  // otherwise, buffer it
        buffer[pos++] = c;
      }
    }
    else {
      if (digitalRead(botaoModo) == HIGH) {
        estadoBotaoModo++;
        if (estadoBotaoModo == 2) {
          estadoBotaoModo = 0;
        }
      }

      if (estadoBotaoModo == 0) {
        Serial.print("s");
        Serial.print(" ");
        Serial.print(valorcal1DHT1 * t1 + valoroff1DHT1);
        Serial.print(" ");
        Serial.print(h1);
        Serial.print(" ");
        Serial.print(valorcal2DHT2 * t2 + valoroff2DHT2);
        Serial.print(" ");
        Serial.print(h2);
        Serial.print(" ");
        Serial.print(valorcal3DHT3 * t3 + valoroff3DHT3);
        Serial.print(" ");
        Serial.print(h3);
        Serial.print(" ");
        Serial.print(t4);
        Serial.print(" ");
        Serial.print(h4);
        Serial.print(" ");
        Serial.print(220);
        Serial.print(" ");
        Serial.println();
      }
      else if (estadoBotaoModo == 1) {
        Serial.print("c");
        Serial.print(" ");
        Serial.print(valorcal1DHT1 * t1 + valoroff1DHT1);
        Serial.print(" ");
        Serial.print(h1);
        Serial.print(" ");
        Serial.print(valorcal2DHT2 * t2 + valoroff2DHT2);
        Serial.print(" ");
        Serial.print(h2);
        Serial.print(" ");
        Serial.print(valorcal3DHT3 * t3 + valoroff3DHT3);
        Serial.print(" ");
        Serial.print(h3);
        Serial.print(" ");
        Serial.print(t4);
        Serial.print(" ");
        Serial.print(h4);
        Serial.print(" ");
        Serial.print(220);
        Serial.print(" ");
        Serial.println();
      }
    }
  }
  Serial.flush();

}
